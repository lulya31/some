System(core, "Core", "Общий функционал", $link="/05_c4/01_core/")

System(coreapp, "Coreapp", "Комментарии, датагриды")

System(comments, "Comments", "", $link="/05_c4/01_core/01_coreapp/01_comments/")
System(annotations, "Annotations", "", $link="/05_c4/01_core/01_coreapp/02_annotations/")
System(datagrid, "Datagrid", "", $link="/05_c4/01_core/01_coreapp/03_datagrids/")
System(filters, "Filters", "", $link="/05_c4/01_core/01_coreapp/04_filers/")

Rel_D(core, coreapp, "")

Rel_D(coreapp, comments, "")
Rel_D(coreapp, annotations, "")
Rel_D(coreapp, datagrid, "")
Rel_D(coreapp, filters, "")

@enduml
